                </binding>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:41210/LabService.asmx" binding="basicHttpBinding" bindingConfiguration="LabServiceSoap" contract="LabServices.LabServiceSoap" name="LabServiceSoap"/>
